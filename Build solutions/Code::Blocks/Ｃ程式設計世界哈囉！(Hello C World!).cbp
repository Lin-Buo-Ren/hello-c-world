<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Ｃ程式設計世界哈囉！(Hello C World!)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="$(TARGET_NAME)/Built software/$(PROJECT_NAME).exe" prefix_auto="0" extension_auto="0" />
				<Option working_dir="../.." />
				<Option object_output="$(TARGET_NAME)/Object code/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="$(TARGET_NAME)/Built software/$(PROJECT_NAME).exe" prefix_auto="0" extension_auto="0" />
				<Option working_dir="../.." />
				<Option object_output="$(TARGET_NAME)/Object code/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pedantic" />
			<Add option="-Wall" />
			<Add option="-ansi" />
		</Compiler>
		<Unit filename="../../Source code/Hello C world!.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
